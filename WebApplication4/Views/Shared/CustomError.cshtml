@model HandleErrorInfo
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Error</title>
</head>
<body>
    <hgroup>
        <h1>Error.</h1>
        <h2>An Custom Error occurred while processing your request.</h2>
        <h2>Inner Exception Controller @Html.DisplayFor(e => e.ControllerName)</h2>
        <h2>Inner Exception Action @Html.DisplayFor(e => e.ActionName)</h2>
    </hgroup>
</body>
</html>
